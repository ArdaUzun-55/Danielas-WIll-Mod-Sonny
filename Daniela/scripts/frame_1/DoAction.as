KrinLang.GERMAN.CUTSUB = new Array();
KrinLang.GERMAN.CUTSUB[0] = "Man nennt mich Sonny.";
KrinLang.GERMAN.CUTSUB[1] = "Ich wrde dir gerne mehr ber mich erzhlen, kann ich aber nicht.";
KrinLang.GERMAN.CUTSUB[2] = "Ich wei noch nicht mal meinen eigenen Namen.";
KrinLang.GERMAN.CUTSUB[3] = "Es begann alles vor einem Jahr, auf diesem schrecklichen Schiff...";
KrinLang.GERMAN.CUTSUB[4] = "Ich werde das Schiff nie vergessen.";
KrinLang.GERMAN.CUTSUB[5] = "Dort bin ich gestorben.";
KrinLang.GERMAN.CUTSUB[6] = "Aber jemand hat mich zurck gebracht.";
KrinLang.GERMAN.CUTSUB[7] = "Aufwachen Sonny! Hoffentlich bist du noch nicht zu weit weg...";
KrinLang.GERMAN.CUTSUB[8] = "...Sonny?";
KrinLang.GERMAN.CUTSUB[9] = "Na also! Vielleicht besteht fr dich doch noch Hoffnung.";
KrinLang.GERMAN.CUTSUB[10] = "Los, Bewegung! Wir mssen dieses Schiff verlassen!";
KrinLang.GERMAN.CUTSUB[11] = "Wer bist du?";
KrinLang.GERMAN.CUTSUB[12] = "Ich bin Louis. Nur ein Freund. Kannst du stehen?";
KrinLang.GERMAN.CUTSUB[13] = "Ja.";
KrinLang.GERMAN.CUTSUB[14] = "Du brauchst ne Waffe! Such dir schnell eine!";
KrinLang.GERMAN.CUTSUB[15] = "Uh...wie wrs mit diesem Rohr?";
KrinLang.GERMAN.CUTSUB[16] = "Ja, gut, nimm es! Hier entlang!";
KrinLang.GERMAN.CUTSUB[17] = "Sonny nimm das.";
KrinLang.GERMAN.CUTSUB[18] = "Louis, ich wei nicht was ich machen soll oder wohin ich gehen kann! Ich kann mich an nichts erinnern.";
KrinLang.GERMAN.CUTSUB[19] = "Nimm es einfach...bitte...";
KrinLang.GERMAN.CUTSUB[20] = "Was ist es?";
KrinLang.GERMAN.CUTSUB[21] = "Es ist eine Kassette. Vielleicht kann es dir weiter helfen.";
KrinLang.GERMAN.CUTSUB[22] = "Louis? Ich brauch keine Kassette! Ich brauche meine Erinnerungen! I muss...";
KrinLang.GERMAN.CUTSUB[23] = "Wir haben es ein paar Tage nachdem wir diesen Paladin besiegt haben erreicht...";
KrinLang.GERMAN.CUTSUB[24] = "Ich dachte es wre die Antwort zu allem. ";
KrinLang.GERMAN.CUTSUB[25] = "Das Licht am Ende des Tunnelsaber ich irrte mich.";
KrinLang.GERMAN.CUTSUB[26] = "Wie ich mich irrtehier begann alles.";
