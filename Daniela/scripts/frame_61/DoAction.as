   Krin.bgElementsInteracted[elementId] = 1;
   var _loc3_ = 0;
   var _loc4_ = 0;
   while(_loc4_ < Krin.bgElementsInteracted.length)
   {
      _loc3_ += Krin.bgElementsInteracted[_loc4_];
      _loc4_ += 1;
   }
   trace("Total BG Element Clicked: " + _loc3_);
   if(_loc3_ >= Krin.bgElementsInteracted.length)
   {
      _root.sendAchievement("ACH_S1_INTERACT_ALL_BG");
   }
};
